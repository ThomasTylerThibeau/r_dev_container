1753448889075:line <- gsub("' ", " ", line)
1753448889075:line <- gsub(" '", " ", line)
1753448889075:}
1753448889075:if (!hyphens)
1753448889076:{ line <- gsub("-", " ", line) }
1753448889076:## this is annoying
